<?xml version="1.0" encoding="UTF-8"?>
<form name='frmTemplatesMinecraft'>
    <template name='caracteristicaLabelEdit'>
        <layout align='client'>
            <label autoSize="true" text='$(text)' align="top"/>
            <edit align="client" max="34" height="$(height)" field="$(field)"/>
        </layout>
    </template>
    <template name='caracteristicaLabelEditNumber'>
        <layout align='client'>
            <label autoSize="true" text='$(text)' align="top"/>
            <edit align="client" height="$(height)" field="$(field)" type="number"/>
        </layout>
    </template>
</form>

<?xml version="1.0" encoding="UTF-8"?>
<frame name='frame_slot' width="30" height="30">
    <borders right="5" top="5" bottom="5" left="5"/>
    <draw>
        <image left="5" top="5" right="25" bottom="25" overflowX="stretch" overflowY="stretch" >
            <anchors left="true" top="true" right="true" bottom="true"/>
            <source url="inv_slot.png" left="120" top="120" right="240" bottom="240"/>
        </image>
        <!-- Cantos-->
        <!-- Superior Esquerdo-->
        <image left="0" top="0" right="5" bottom="5">
            <anchors left="true" top="true"/>
            <source url="inv_slot.png" left="0" top="0" right="120" bottom="120"/>
        </image>
        <!-- Superior Direito-->
        <image left="25" top="0" right="30" bottom="5">
            <anchors right="true" top="true"/>
            <source url="inv_slot.png" left="240" top="0" right="360" bottom="120"/>
        </image>
        <!-- Inferior Esquerdo-->
        <image left="0" top="25" right="5" bottom="30">
            <anchors left="true" bottom="true"/>
            <source url="inv_slot.png" left="0" top="240" right="120" bottom="360"/>
        </image>
        <!-- Inferior Direito-->
        <image left="128" top="128" right="192" bottom="192">
            <anchors right="true" bottom="true"/>
            <source url="inv_slot.png" left="128" top="128" right="192" bottom="192"/>
        </image>
        <!-- Paredes-->
        <!-- Esquerdo-->
        <image left="0" top="64" right="64" bottom="128" zOrder="-1" overflowY="tile">
            <anchors left="true" right="false" bottom="true" top="true"/>
            <source url="inv_slot.png" left="0" top="64" right="64" bottom="128"/>
        </image>
        <!-- Superior-->
        <image left="64" top="0" right="128" bottom="64" zOrder="-1" overflowX="tile">
            <anchors left="true" right="true" top="true"/>
            <source url="inv_slot.png" left="64" top="0" right="128" bottom="64"/>
        </image>
        <!-- Inferior-->
        <image left="64" top="128" right="128" bottom="192" zOrder="-1" overflowX="tile">
            <anchors bottom="true" left="true" right="true" top="false" />
            <source url="inv_slot.png" left="64" top="128" right="128" bottom="192"/>
        </image>
        <!-- Direito-->
        <image left="128" top="64" right="192" bottom="128" zOrder="-1" overflowY="tile">
            <anchors left="false" right="true" bottom="true" top="true"/>
            <source url="inv_slot.png" left="128" top="64" right="192" bottom="128"/>
        </image>
    </draw>
</frame>    